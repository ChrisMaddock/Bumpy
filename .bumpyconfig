# Captures AssemblyVersion and AssemblyFileVersion in all AssemblyInfo.cs files
[assembly]
Source\Bumpy\AssemblyInfo.cs | UTF-8 = (?<version>\d+\.\d+\.\d+\.\d+)

# Captures the version tag in all .nuspec files
[nuspec]
NuSpec\*.nuspec = <version>(?<version>\d+\.\d+\.\d+)

# Captures the variable "milestone" in the build file
[cake]
Source\build.cake = "(?<version>\d+\.\d+\.\d+)"
